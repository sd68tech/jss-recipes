<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Created with Recipe Robot v2.2.0 (https://github.com/homebysix/recipe-robot)</string>
	<key>Description</key>
	<string>Downloads the latest version of UPDD Commander.</string>
	<key>Identifier</key>
	<string>com.github.sd68tech.download.UPDDCommander</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>UPDD Commander</string>
	</dict>
	<key>MinimumVersion</key>
	<string>1.0.0</string>
	<key>Process</key>
	<array>
